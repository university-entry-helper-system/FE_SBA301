# API Configuration
# Base URL for API requests (e.g., http://localhost:8080/api)
VITE_API_BASE_URL=http://localhost:8080/api

# API request timeout in milliseconds (default: 30 seconds)
VITE_API_TIMEOUT=30000

# Application Configuration
# Name of the application
VITE_APP_NAME="SBA Frontend"

# Current environment (development/staging/production)
VITE_APP_ENV=development

# Authentication Configuration
# LocalStorage key for storing the authentication token
VITE_AUTH_TOKEN_KEY=auth_token

# LocalStorage key for storing the refresh token
VITE_REFRESH_TOKEN_KEY=refresh_token